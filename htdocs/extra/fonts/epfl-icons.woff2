          .css({'display': 'none', 'visibility': 'hidden'});
        var hash = window.location.hash
        $('.nav-item-active', '.mainnav, .localnav').removeClass('nav-item-active');
        if (slug) {
          var link = $('.localnav .nav-link[href="'+slug[1]+'.html'+hash+'"]');
          if (link.length) {
            var item = link.closest('.nav-item').addClass('nav-item-active');
            $('.localnav .nav-item[data-category="'+item.attr('data-category')+'"]')
              .attr('aria-hidden', 'false')
              .css({'display': 'inherit', 'visibility': 'visible'});
          }
          link = $('.mainnav .nav-link[href="'+slug[1]+'.html'+hash+'"]');
          if (link.length) {
            var $item = link.closest('.nav-item:not(.hidden-xs)').addClass('nav-item-active');
            link.parents('.nav-item').last().addClass('nav-item-parent');
            if (!$item.attr('id')) {
              $item.attr('id', _.getGUID('nav-item-'));
            }
            $('.mainnav [role="menubar"]').attr('aria-activedescendant', $item.attr('id'));
          }
        }
        $('[role="tree"]').epfl_tree('selectItem', $('[role="tree"] .nav-item-active'));
      };
      updateNav();
      $(window).on('hashchange', function() { updateNav(); });
      
      window.setTimeout(function() {
        _.LazyLoad.load();
      })
      // Toggle code examples on click
      $(document).on('click', '.code, .template', function(e){ $(this).toggleClass('expanded').children().toggle(); })
      $(document).on('click', '.code pre, .template pre', function(e) { e.stopPropagation(); });
      // Show background color in table cells
      $('.table.bgColor').find('td span, td code').each(function(){
        var $this = $(this);
        var bgColor = $this.css('background-color');
        var hexColor = bgColor.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/).slice(1,4).map(function(x){
          return ('0' + parseInt(x).toString(16)).slice(-2);
        }).join('');
        $this.html('&nbsp;#' + hexColor + '&nbsp;');
      });
    });
    // Overwrite local site search
    require(['search'], function(s){
      $('#epfl-header').epfl_search('update', {engines:{site:{url: 'search.html',ac: false}}});
    });
  });
  
</script>

</body>
</html>
