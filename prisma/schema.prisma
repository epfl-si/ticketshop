generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./ticketshop.db"
}

model Funds {
  id            Int     @id @default(autoincrement())
  resourceId    String @unique
  cf            String
  users         Users[]
  dfs           Dfs[]
}

model Users {
  id            Int     @id @default(autoincrement())
  sciper        Int @unique
  createdAt     DateTime @default(now())
  funds         Funds[]
  dfs           Dfs[]
}

model Dfs {
  id            Int     @id @default(autoincrement())
  requestID     Int
  name          String
  user          Users[]
  dates         String
  destination   String
  fund          Funds[]
}